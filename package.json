{
  "name": "pico-go",
  "displayName": "Pico-Go",
  "description": "Adds a REPL console to the terminal that connects to your Raspberry Pi Pico board. Run single files or synchronize your project files to your board.",
  "version": "1.2.0",
  "publisher": "ChrisWood",
  "homepage": "http://pico-go.net/",
  "repository": "https://github.com/cpwood/Pico-Go",
  "icon": "images/logo.png",
  "license": "SEE LICENSE IN <LICENSE.md>",
  "xtype": "module",
  "galleryBanner": {
    "color": "#ffffff",
    "theme": "light"
  },
  "engines": {
    "vscode": "^1.53.0"
  },
  "keywords": [
    "raspberry",
    "pi",
    "pico"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./pymakr",
  "compatibility": [
    {
      "platform": "win32",
      "arch": [
        "x64"
      ]
    },
    {
      "platform": "darwin",
      "arch": [
        "x64",
        "arm64"
      ]
    },
    {
      "platform": "linux",
      "arch": [
        "x64",
        "arm64"
      ]
    }
  ],
  "contributes": {
    "commands": [
      {
        "command": "pymakr.connect",
        "title": "Pico-Go > Connect"
      },
      {
        "command": "pymakr.disconnect",
        "title": "Pico-Go > Disconnect"
      },
      {
        "command": "pymakr.initialise",
        "title": "Pico-Go > Configure project"
      },
      {
        "command": "pymakr.upload",
        "title": "Pico-Go > Upload project"
      },
      {
        "command": "pymakr.uploadFile",
        "title": "Pico-Go > Upload current file only"
      },
      {
        "command": "pymakr.download",
        "title": "Pico-Go > Download project"
      },
      {
        "command": "pymakr.run",
        "title": "Pico-Go > Run current file"
      },
      {
        "command": "pymakr.runselection",
        "title": "Pico-Go > Run current selection"
      },
      {
        "command": "pymakr.deleteAllFiles",
        "title": "Pico-Go > Delete all files from board"
      },
      {
        "command": "pymakr.globalSettings",
        "title": "Pico-Go > Global settings"
      },
      {
        "command": "pymakr.reset.soft",
        "title": "Pico-Go > Reset > Soft"
      },
      {
        "command": "pymakr.reset.hard",
        "title": "Pico-Go > Reset > Hard"
      },
      {
        "command": "pymakr.extra.pins",
        "title": "Pico-Go > Extras > Show Pico Pin Map"
      },
      {
        "command": "pymakr.extra.getSerial",
        "title": "Pico-Go > Extras > List serial ports"
      },
      {
        "command": "pymakr.extra.getFullVersion",
        "title": "Pico-Go > Extra > Get support info"
      },
      {
        "command": "pymakr.help",
        "title": "Pico-Go > Help"
      },
      {
        "command": "pymakr.listCommands",
        "title": "Pico-Go > List all Commands"
      }
    ],
    "keybindings": [
      {
        "command": "",
        "key": "ctrl+e",
        "when": "terminalFocus"
      },
      {
        "command": "pymakr.initialise",
        "key": "ctrl+shift+i",
        "mac": "cmd+shift+i"
      },
      {
        "command": "pymakr.connect",
        "key": "ctrl+shift+c",
        "mac": "cmd+shift+c"
      },
      {
        "command": "pymakr.upload",
        "key": "ctrl+shift+u",
        "mac": "cmd+shift+u"
      },
      {
        "command": "pymakr.uploadFile",
        "key": "ctrl+shift+f",
        "mac": "ctrl+shift+f"
      },
      {
        "command": "pymakr.run",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r"
      },
      {
        "command": "pymakr.runselection",
        "key": "ctrl+shift+enter",
        "mac": "cmd+shift+enter"
      },
      {
        "command": "pymakr.globalSettings",
        "key": "ctrl+shift+g",
        "mac": "cmd+shift+g"
      },
      {
        "command": "pymakr.reset.soft",
        "key": "ctrl+shift+d",
        "mac": "cmd+shift+d"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "pymakr.upload",
          "group": "pymakr"
        },
        {
          "command": "pymakr.run",
          "group": "pymakr"
        }
      ],
      "editor/context": [
        {
          "command": "pymakr.connect",
          "group": "pymakr"
        },
        {
          "command": "pymakr.run",
          "group": "pymakr"
        },
        {
          "command": "pymakr.runselection",
          "group": "pymakr"
        },
        {
          "command": "pymakr.upload",
          "group": "pymakr"
        }
      ],
      "editor/title": [
        {
          "command": "pymakr.run",
          "group": "pymakr"
        },
        {
          "command": "pymakr.upload",
          "group": "pymakr"
        }
      ]
    }
  },
  "statusBar": [
    {
      "key": "status",
      "name": "",
      "command": "pymakr.toggleConnect",
      "tooltip": "Toggle board connection"
    },
    {
      "key": "run",
      "name": "$(triangle-right) Run",
      "command": "pymakr.run",
      "tooltip": "Run current file"
    },
    {
      "key": "runselection",
      "name": "$(triangle-right) Run Line",
      "command": "pymakr.runselection",
      "tooltip": "Run current line"
    },
    {
      "key": "upload",
      "name": "$(triangle-up) Upload file",
      "command": "pymakr.uploadFile",
      "tooltip": "Upload current file to your board"
    },
    {
      "key": "download",
      "name": "$(triangle-down) Download",
      "command": "pymakr.download",
      "tooltip": "Download project from your board"
    },
    {
      "key": "disconnect",
      "name": "$(chrome-close) Disconnect",
      "command": "pymakr.disconnect",
      "tooltip": "Disconnect"
    },
    {
      "key": "softreset",
      "name": "$(refresh) Reset",
      "command": "pymakr.reset.soft",
      "tooltip": "Restart your board"
    },
    {
      "key": "settings",
      "name": "$(gear) Settings",
      "command": "pymakr.globalSettings",
      "tooltip": "Global Pico-Go settings"
    },
    {
      "key": "getversion",
      "name": "$(tag) Get version",
      "command": "pymakr.extra.getVersion",
      "tooltip": "Get firmware version"
    },
    {
      "key": "listserial",
      "name": "$(list-unordered) List serialports",
      "command": "pymakr.extra.getSerial",
      "tooltip": "List available serialports"
    },
    {
      "key": "listcommands",
      "name": "$(list-unordered) All commands",
      "command": "pymakr.listCommands",
      "tooltip": "List all available Pico-Go commands"
    }
  ],
  "scripts": {
    "changelog": "github_changelog_generator -u cpwood -p Pico-Go --future-release v$npm_package_version",
    "copy-native": "node native_modules/copy-native.js",
    "build": "babel src --out-dir lib --source-maps",
    "build-dev": "babel src --out-dir lib --source-maps --watch",
    "postinstall": "node native_modules/copy-native.js",
    "package": "vsce package",
    "vscode:prepublish": "npm ci && npm prune"
  },
  "dependencies": {
    "binascii": "0.0.2",
    "bindings": "^1.5.0",
    "child-process-promise": "^2.2.1",
    "commander": "4.0.1",
    "copy-paste": "^1.3.0",
    "crypto-js": "^3.3.0",
    "debug": "^4.3.1",
    "electron": "^6.1.12",
    "electron-rebuild": "^1.11.0",
    "element-resize-detector": "1.1.15",
    "fs-extra": "^9.1.0",
    "lie": "^3.3.0",
    "lodash": "^4.17.20",
    "nan": "2.14.0",
    "ncp": "^2.0.0",
    "node-abi": "^2.19.3",
    "object.assign": "^4.1.2",
    "ora": "^4.1.1",
    "promirepl": "^2.0.1",
    "prompt-list": "^3.2.0",
    "rxjs": "^6.6.3",
    "safe-buffer": "^5.2.1",
    "serialport": "^9.0.6",
    "spdx-correct": "^3.1.1",
    "spdx-exceptions": "^2.3.0",
    "telnet-client": "^1.4.6",
    "typescript": "^3.9.7",
    "utf8": "^3.0.0",
    "xterm": "^4.10.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.19.16",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "6.24.1",
    "chai": "^4.3.0",
    "eslint": "^6.8.0",
    "mocha": "^6.2.3",
    "prebuild-install": "^5.3.6",
    "vscode": "^1.1.37"
  }
}
